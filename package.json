{
  "name": "buddyforms",
  "description": "Contact Forms, Post Forms for User Generated Content and Registration Forms easily build in minutes. Ideal for User Submitted Posts. Extendable with Addons!",
  "author": "ThemeKraft Team.",
  "license": "GPLv2 or later",
  "homepage": "https://themekraft.com/buddyforms/",
  "version": "2.5.14",
  "dependencies": {},
  "scripts": {
    "build": "npm run css-build && npm run custom-prefix-buddyforms && npm run custom-prefix-cleancss-buddyforms && npm run custom-prefix-sourcemap-buddyforms && npm run custom-prefix-buddyforms-editor && npm run custom-prefix-cleancss-buddyforms-editor",
    "css-build": "node-sass --output-style expanded --no-source-map -r scss -o assets && npm run custom-prefix-buddyforms && npm run custom-prefix-buddyforms-editor && npm run custom-prefix-user-satisfaction",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch",
    "build-buddyforms-editor-extra": "node-sass --output-style expanded scss/admin/css/buddyforms-editor-extra.scss assets/admin/css/buddyforms-editor-extra.css",
    "watch-buddyforms-editor-extra": "nodemon -e scss -x \"npm run build-buddyforms-editor-extra\"",
    "custom-prefix-buddyforms": "postcss assets/css/buddyforms.css --no-map --replace true --verbose",
    "custom-prefix-buddyforms-editor": "postcss assets/admin/css/buddyforms-editor.css --no-map --replace true --verbose",
    "custom-prefix-user-satisfaction": "postcss assets/admin/css/user-satisfaction.css --no-map --replace true --verbose",
    "custom-prefix-cleancss-buddyforms": "cleancss -o assets/css/buddyforms.min.css assets/css/buddyforms.css",
    "custom-prefix-cleancss-buddyforms-editor": "cleancss -o assets/admin/css/buddyforms-editor.min.css assets/admin/css/buddyforms-editor.css",
    "custom-prefix-cleancss-user-satisfaction": "cleancss -o assets/admin/css/user-satisfaction.min.css assets/admin/css/user-satisfaction.css"
  },
  "repository": "BuddyForms/BuddyForms.git",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=6.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "bulma": "^0.9.1",
    "bulma-checkradio": "^1.1.1",
    "clean-css-cli": "^4.3.0",
    "font-awesome": "^4.7.0",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.6",
    "postcss": "^7.0.30",
    "postcss-cli": "^7.1.1",
    "postcss-prefixer": "^2.1.2",
    "rimraf": "^3.0.2"
  }
}
